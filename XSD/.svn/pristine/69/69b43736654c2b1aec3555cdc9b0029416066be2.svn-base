#mapStyle .changeBaseMapBtn span {
    font-size: 12px;
    color: #fff;
    padding: 3px;
    border-radius: 3px;
    background-color:#20A0FF;
}

.changeBaseMapBtn{
    position: absolute;
    top: 20px;
    right: 20px;
    width: 85px;
    height: 60px;
    background: url(../images/basevec.png) no-repeat 0 0;
    background-size: 86px 120px;
    border-radius: 3px;
    transition: all .5s ease;
    z-index: 2000;
}

