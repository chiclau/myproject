	// 字符串切割
	public static String splitCity(String city){
		String[] split = city.split(",");
		String str = "";
		for (int i = 0; i < split.length; i++) {
			if(i == split.length-1){
				str += "'"+split[i]+"'";
			}else{
				str += "'"+split[i]+"',";
			}
		}
		return str;
	}

