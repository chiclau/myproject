            if(formBean.getSjly().equals("1") ) {//水利部
          	  sql.append("AND  hps.slb_dzbh_flag=1  ");
	          }
            if (formBean.getSjly().equals("2") ) {//环境部
	        	  sql.append("AND  hps.sthjb_dzbh_flag=2  ");
	  		}
            if (formBean.getSjly().equals("3")) {//能源局
	  			 sql.append("AND   hps.nyj_dzbh_flag=3 ");
	  		}
        }
        if(formBean.getXmmc()!=null && !"".equals(formBean.getXmmc())){ // 项目名称
            sql.append("and hps.xmmc like '%" + formBean.getXmmc() + "%' ");
        }
        if(formBean.getSheng()!=null && !"".equals(formBean.getSheng())){ // 省
            sql.append("and hps.sheng in ("+ this.splitCity(formBean.getSheng()) + ") or hps.shi in ("+ this.splitCity(formBean.getSheng()) + ") or hps.xian in ("+ this.splitCity(formBean.getSheng()) + ") ");
