<%@ page language="java" pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>

<div class="modal fade" id="change_dialog">
	<div class="modal-dialog modal-lg" style="width: 400px;">
		<div class="modal-content">
			<div class="modal-header"
				style="height: 40px">
				<button type="button" class="btn btn-link close"
					data-dismiss="modal">
					<span aria-hidden="true"><i class="icon icon-times"></i></span><span class="sr-only">关闭</span>
				</button>
				<h4 class="modal-title" style="line-height: 15px;"><i class="icon icon-refresh"></i>&nbsp;请选择工程</h4>
			</div>
				<div style="display:none;">
					工程内码<input type="text" class="form-control" id="eng_nm" name="">
					工程名称<input type="text" class="form-control" id="eng_name" name="">
				</div>

			<div class="modal-body" style="padding-top: 5px;">
				<div id="zTreeDiv" class="zTreeDiv">
					<ul id="eng_tree" class="ztree"></ul>
				</div>
			</div>
			<div class="modal-footer"
				style="height: 40px;">
				<button type="button" class="btn btn-primary" onclick="saveChange()" 
					style="margin-top: -14px;">
					<i class="icon icon-download-alt"></i> 确定
				</button>
				<button type="button" class="btn btn-large"
					data-dismiss="modal" style="margin-top: -14px; margin-right: -5px">
					<i class="icon icon-times"></i> 取消
				</button>
			</div>
		</div>
	</div>
</div>

