<script type="text/javascript">
	$(".form_datetime").datetimepicker({
		format : " yyyy-mm-dd hh:ii:ss"
	});
	$("#test_001").load("mainList4.jsp",function(response,status,xhr){   //div的id用来load刷新   请求后台
		if(status!="success"){
			if(xhr.status==444){
				alert(xhr.responseText);
			}else{
				alert(status);
			}
		}
	});
	var i = 0;
	function clicktr(){
		if(i == 0){
			$("#test_001").hide(1000);
			i = 1;
		}else{
			$("#test_001").show(100);
			i = 0;
		}
	}
</script>
